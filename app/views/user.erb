<div class="container">
  <h1>Welcome, User #<%= @user.id %></h1>
  
  <h4>Pick a Deck...</h4>
  <ul>
    <% @decks.each do |deck| %>
    <li> <a href="/user/<%=@user.id%>/deck/<%=deck.id%>"><%= deck.name %> </a></li>
    <% end %> 
  </ul>

  <p><a href ="/user/<%=@user.id%>/history">view my history</a></p>
</div>

